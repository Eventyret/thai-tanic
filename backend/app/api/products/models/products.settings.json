{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Products"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "soup",
        "salad",
        "curry",
        "noodles",
        "rice"
      ]
    },
    "image": {
      "type": "string"
    }
  }
}
